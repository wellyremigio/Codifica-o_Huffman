/*
 * Classe abstrata que representa a √Årvore.
 */
abstract class Arvore implements Comparable<Arvore> {
    public final int frequencia; 
  
    public Arvore(int freq) { 
    	frequencia = freq; 
    }
    
    public int compareTo(Arvore tree) {
        return frequencia - tree.frequencia;
    }
}
